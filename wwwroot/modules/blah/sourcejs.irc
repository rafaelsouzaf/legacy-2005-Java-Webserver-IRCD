}

function privado(alguem) {
	// se usuario abriu pvt com ele mesmo
	if (alguem == this.meuNick) {
		main(alguem, '<font color=#008000>*** Iniciado chat privado com ' + alguem + '.</font><br>', '1', '1');
		main(alguem, '<font color=#FF0000>*** Oops! Você abriu um chat privado com você mesmo.</font><br>', '1', '1');
	} else {
		//comando('/whois ' + alguem);
		main(alguem, '<font color=#008000>*** Iniciado chat privado com ' + alguem + '.</font><br>', '1', '1');
		main(alguem, '<font color=#008000>*** Para voltar a sala onde você se encontra, clique nas abas acima.</font><br>', '1', '1');
	}
}

