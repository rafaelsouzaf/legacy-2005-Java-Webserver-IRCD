function expulsar(alguem) {

	// se alguem for igual ao seu proprio nick
	// entao avisa que está tentando expulsar a si mesmo
	if (alguem == this.meuNick) {
		main(this.janelaAtual, '<font color=#FF0000>*** Oops! Você está tentando expulsar a si mesmo.</font><br>', '1', '1');
	} else {
	    motivo = prompt("Digite o motivo porque está expulsando o usuário " + alguem + ":", "Motivo não consta.");
	    if (motivo != null)
			comando('/kick ' + this.janelaAtual + ' ' + alguem + ' :' + motivo);
	}
}

function banir(alguem) {

	// se alguem for igual ao seu proprio nick
	// entao avisa que está tentando expulsar a si mesmo
	if (alguem == this.meuNick){

		main(this.janelaAtual, '<font color=#FF0000>*** Oops! Você está tentando banir a si mesmo.</font><br>', '1', '1');

	} else {

		comando('/mode set ban ' + this.janelaAtual + ' ' + alguem);
		expulsar(alguem);

	}

}

